{
  "name": "url-shortener-fastify",
  "packageManager": "yarn@4.10.3",
  "version": "0.1.0",
  "type": "module",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "tsc -p .",
    "start": "node dist/index.js",
    "lint": "eslint . --ext .ts",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate dev --name init",
    "db:push": "prisma db push",
    "seed": "tsx prisma/seed.ts",
    "test:load": "k6 run k6/redirect-load.js"
  },
  "devDependencies": {
    "@types/node": "^24.5.2",
    "prisma": "^6.16.2",
    "tsx": "^4.20.5"
  },
  "dependencies": {
    "@fastify/cors": "^11.1.0",
    "@fastify/rate-limit": "^10.3.0",
    "@prisma/client": "^6.16.2",
    "fastify": "^5.6.1",
    "ioredis": "^5.8.0",
    "nanoid": "^5.1.6",
    "zod": "^4.1.11"
  }
}
